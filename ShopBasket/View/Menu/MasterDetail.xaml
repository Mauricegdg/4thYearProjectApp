<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:Local="clr-namespace:ShopBasket.View.Menu;assembly=ShopBasket"
             xmlns:DetailViews="clr-namespace:ShopBasket.View.DetailViews;assembly=ShopBasket"
             mc:Ignorable="d"
             x:Class="ShopBasket.View.Menu.MasterDetail"
                  BackgroundImageSource="background2.jpg">
    <MasterDetailPage.Master>
        <Local:MasterPage x:Name="masterpage" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
         <x:Arguments>
            <DetailViews:Home />
         </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>